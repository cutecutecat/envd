def build():
    base(dev=True)
    install.conda()
    install.python()
    config.conda_channel(
        channel="""
channels:
  - defaults
  - bioconda #  bioconda-repodata-patches in env.yaml only works if specify channels here
    """
    )
    install.conda_packages(env_file="env.yaml")
